{
    "id": "network-indicator",
    "name": "NetworkIndicator",
    "version": "0.0.1",
    "minNoctaliaVersion": "3.6.0",
    "author": "tonigineer",
    "license": "MIT",
    "repository": "https://github.com/tonigineer/noctalia-plugins",
    "description": "A lively network speed indicator for RX/TX traffic.",
    "entryPoints": {
        "barWidget": "BarWidget.qml",
        "settings": "Settings.qml"
    },
    "dependencies": {
        "plugins": []
    },
    "metadata": {
        "defaultSettings": {
            "arrowType": "caret",
            "byteThresholdActive": 1024,
            "fontSizeModifier": 0.75,
            "forceMegabytes": false,
            "iconSizeModifier": 1.0,
            "minWidth": 0,
            "showNumbers": false,
            "spacingInbetween": 0,
            "useCustomColors": false
        }
    }
}
